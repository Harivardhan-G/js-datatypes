    <script>
        for(i=1;i<=5;i++){
            document.write(i,"<br>");
        }
        for(j=2;j<=9;j=j+2){
            document.write(j+3,"<br>");   
            //i=2;
            //c=2<=9, 4<=9, 6<=9; 8<=9;
            //u= i+2; 2+2=4; 4+2=6; 6+2=9; 8+2=10;
            //b=2+3=5; 4+3=7;  6+3=9; 8+3=11;
        }
        for(i=1;i<=5;i++){
            document.write("hari"+"<br>")
        }
        let m=9
        for(i=1;i<=10;i++){
            document.write("9*"+i+"="+m*i+"<br>")
        }
        const numbers=[1,2,3,4,5]
        let sum=0
        for(let i=0;i<numbers.length;i++){
            sum=sum+numbers[i]
        }
    </script>
